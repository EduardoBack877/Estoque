	<queryString>
		<![CDATA[SELECT m.periodo, p.descricao, m.qtd, m.operacao
FROM produto p
	LEFT JOIN movestoque m ON 
	 p.id = m.codprod 
WHERE 
	 m.periodo BETWEEN  CAST($P{periodoini} AS TIMESTAMP) AND CAST($P{periodofi} AS TIMESTAMP) AND m.operacao = $P{operacao} ]]>
